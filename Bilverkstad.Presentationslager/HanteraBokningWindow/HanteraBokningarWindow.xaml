<Window x:Class="Bilverkstad.Presentationslager.HanteraBokningarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bilverkstad.Presentationslager"
        mc:Ignorable="d"
        Title="HanteraBokningWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Skapa bokning" HorizontalAlignment="Left" Margin="16,96,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.505,0.603" Click="Button_Click"/>
        <Button Content="Ändra bokning" HorizontalAlignment="Left" Margin="16,132,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <DataGrid x:Name="Bokningar" Margin="123,51,0,0" AutoGenerateColumns="True" SelectionMode="Single" SelectionUnit="FullRow"/>
        <TextBox x:Name="txtSök" HorizontalAlignment="Left" Margin="123,16,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Sök" HorizontalAlignment="Left" Margin="261,14,0,0" VerticalAlignment="Top" Click="SökButton_Click"/>
    </Grid>

</Window>
