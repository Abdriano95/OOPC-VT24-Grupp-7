        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bilverkstad.Presentationslager"
        mc:Ignorable="d"
        Title="HanteraBokningMekanikerWindow" Height="450" Width="800">
    <Grid>

        <Button Content="Öppna bokning" HorizontalAlignment="Left" Margin="21,217,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <DataGrid  x:Name= "Bokningar" Margin="123,0,0,0"/>
        <TextBox HorizontalAlignment="Left" x:Name="txtAnställningsNummer" Margin="21,40,0,0" TextWrapping="Wrap" Text="AnstNr" VerticalAlignment="Top" Width="87"/>
        <ComboBox HorizontalAlignment="Left" x:Name="cmbBokning" SelectionChanged="ComboBox_SelectionChanged" Margin="21,69,0,0" VerticalAlignment="Top" Width="87"/>
    </Grid>
</Window>
